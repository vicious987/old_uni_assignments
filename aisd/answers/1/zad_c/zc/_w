#include <stdio.h>
#include <iostream>
#include <deque>


int main()
{
	//std::deque <int[3]> x;
	//x.push_back({0,0,0});
	std::deque < int > dane;
    dane.push_back( 5 );
    dane.push_back( 2 );
    dane.push_back( 3 );
    dane.push_back( 1 );
    dane.push_front( 4 );
   
    for( size_t i = 0; i < dane.size(); i++ )
         printf( "%d, ", dane[ i ] );
   
    printf( "\n\nKoniec\n" );
    return 0;
//	return 0;
}

