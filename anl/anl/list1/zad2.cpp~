#include <stdio.h>

double recsec(int n)
{
	if(n == 0)
		return 1;
	if(n == 1)
		return 1.0/5.0;
	else
		return 26.0/5.0 * recsec(n-1) - recsec(n-2);
}


int main()
{
	printf("start\n");
	for(int i = 0; i < 20; i++){
		printf("%.20f %d\n", recsec(i), i);
	}
}/*
